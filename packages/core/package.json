{
  "name": "@moonbasehq/js-core",
  "version": "0.0.0-canary-0",
  "exports": {
    ".": "./src/index.ts"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
