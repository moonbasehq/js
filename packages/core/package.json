  },
  "devDependencies": {
    "@moonbasehq/eslint-config": "workspace:*",
    "@moonbasehq/typescript-config": "workspace:*",
    "@turbo/gen": "^1.11.3",
    "@types/eslint": "^8.56.1",
    "@types/node": "^20.10.6",
    "@types/node-fetch": "^2.6.10",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "openapi-fetch": "^0.8.2"
  }
}